    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.set": "^4.3.6",
    "@types/node": "^13.11.1",
    "@types/node-telegram-bot-api": "^0.40.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^6.8.0",
    "prettier": "^2.0.4",
